{
  "name": "@theaterjs/app",
  "private": true,
  "version": "0.1.0-dev.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p . -c tslint.json ./src/**/*.tsx",
    "test": "jest",
    "prettify-and-lint": "run-s prettify lint",
    "prettify": "prettier --write \"src/**/*.tsx\"",
    "prepush": "run-p prettify-and-lint test",
    "lb:webpack:dev": "webpack --progress --config webpack/lb/development.babel.js --watch",
    "lb:dev": "nodemon -e jade --exec \"electron\" ./bundles/lb/index.js",
    "lb:run": "electron ./bundles/lb/index.js",
    "lf:webpack:dev": "webpack-dev-server --progress --config webpack/lf/development.babel.js",
    "studio:webpack:dev": "webpack-dev-server --progress --config webpack/studio/development.js",
    "playground:webpack:dev": "webpack-dev-server --progress --config webpack/playground/development.js",
    "examples:webpack:dev": "webpack-dev-server --progress --config webpack/examples/development.js",
    "build:dev": "npm-run-all --parallel lb:webpack:dev lf:webpack:dev studio:webpack:dev examples:webpack:dev",
    "open-examples": "opn http://localhost:9092/examples"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleNameMapper": {
      "\\$root/(.*)": "<rootDir>/$1",
      "\\$src/(.*)": "<rootDir>/src/$1",
      "\\$lb/(.*)": "<rootDir>/src/lb/$1",
      "\\$lf/(.*)": "<rootDir>/src/lf/$1",
      "\\$studio/(.*)": "<rootDir>/src/studio/$1",
      "\\$shared/(.*)": "<rootDir>/src/shared/$1"
    },
    "setupFiles": [
      "./src/test/env.tsx"
    ],
    "automock": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/theaterjs/app.git"
  },
  "author": "TheaterJS Oy",
  "license": "Proprietary",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/fs-extra": "^5.0.0",
    "@types/html-webpack-plugin": "^2.30.1",
    "@types/jest": "^22.1.0",
    "@types/lodash": "^4.14.93",
    "@types/ramda": "^0.25.16",
    "@types/react-broadcast": "^0.6.0",
    "@types/react-dom": "^16.0.3",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/recompose": "^0.24.4",
    "@types/redux-actions": "^2.2.3",
    "@types/uuid": "^3.4.3",
    "@types/webpack": "^3.8.2",
    "@types/when": "^2.4.29",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
