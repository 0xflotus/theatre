    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "deep-equal": "^1.0.1",
    "dtslint": "^0.2.0",
    "electron": "^1.6.11",
