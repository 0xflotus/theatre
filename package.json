{
  "name": "theatrejs",
  "version": "0.3.0-beta.1",
  "description": "(to be) a timeline-based animation editor for the web",
  "main": "lib/Theatre.js",
  "scripts": {
    "test": "mocha \"test/**/*.coffee\"",
    "test:watch": "mocha \"test/**/*.coffee\" --watch",
    "compile": "coffee --bare --compile --output ./lib ./src",
    "compile:watch": "jitter src lib -b",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AriaMinaei/theatrejs.git"
  },
  "keywords": [
    "keyframes",
    "animation",
    "webgl",
    "timeline"
  ],
  "author": "Aria Minaei",
  "license": "N/A",
  "bugs": {
    "url": "https://github.com/AriaMinaei/theatrejs/issues"
  },
  "dependencies": {
    "audio-driven-time-control": "~0.1.0-beta.8",
    "foxie": "~0.1.13",
    "kilid": "0.1.0-beta.11",
    "moosh": "~0.2.0",
    "socket.io": "~0.9.16",
    "theatrejs-data-server": "~0.1.0-dev.2",
    "timing-function": "^0.2.3",
    "utila": "~0.4.0",
    "when": "~2.7.1"
  },
  "devDependencies": {
    "coffee-script": "~1.7.1",
    "chai": "^1.10.0",
    "chai-changes": "^1.3.4",
    "chai-fuzzy": "^1.4.0",
    "mocha": "^2.0.1",
    "mocha-pretty-spec-reporter": "0.1.0-beta.1",
    "sinon": "^1.12.2",
    "sinon-chai": "^2.6.0"
  }
}
