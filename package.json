    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "exec-loader": "^3.0.0",
    "fast-json-patch": "^2.0.6",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.53.1",
    "flow-typed": "^2.1.5",
    "fs-extra": "^4.0.0",
    "html-webpack-plugin": "^2.29.0",
