{
  "name": "@theatrejs/app",
  "private": true,
  "version": "0.1.18",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p . -t stylish -c tslint.json",
    "test": "jest",
    "prettify-and-lint": "run-s prettify lint",
    "prettify": "prettier --write \"src/**/*.tsx\"",
    "lb:webpack:dev": "webpack --progress --config webpack/lb/development.js --watch",
    "lb:dev": "nodemon --watch ./bundles/lb/index.js --exec \"electron\" ./bundles/lb/index.js",
    "lb:debug": "electron --inspect-brk=9239 ./bundles/lb/index.js",
    "lb:run": "electron ./bundles/lb/index.js",
    "lf:webpack:dev": "webpack-dev-server --progress --config webpack/lf/development.js",
    "studio:webpack:dev": "webpack-dev-server --progress --config webpack/studio/development.js",
    "tl:webpack:dev": "webpack-dev-server --progress --config webpack/tl/development.js",
    "tl:webpack:prod": "webpack --progress --config webpack/tl/production.js",
    "tl:publish": "cross-env NODE_ENV=production run-s tl:webpack:prod tl:publish:prepare",
    "tl:publish:emit-decs": "tsc -p ./tsconfig.publish.json ",
    "tl:publish:prepare": "node ./scripts/tl/prepareBundle.js",
    "playground:webpack:dev": "webpack-dev-server --progress --config webpack/playground/development.js",
    "examples:webpack:dev": "webpack-dev-server --progress --open --config webpack/examples/development.js",
    "build:dev": "npm-run-all --parallel lb:webpack:dev lf:webpack:dev studio:webpack:dev examples:webpack:dev",
    "ci:check-types": "tsc -p ./ci.tsconfig.json --noEmit",
    "ci:check-types:watch": "tsc -p ./ci.tsconfig.json --noEmit --watch",
    "ci:type-tests": "tslint -c ./typeTests.tslint.json -p ./typeTests.tsconfig.json --format stylish ./src/**/*.typeTest.ts*",
    "prepush": "run-p ci:check-types test"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleNameMapper": {
      "\\$root/(.*)": "<rootDir>/$1",
      "\\$src/(.*)": "<rootDir>/src/$1",
      "\\$lb/(.*)": "<rootDir>/src/lb/$1",
      "\\$lf/(.*)": "<rootDir>/src/lf/$1",
      "\\$studio/(.*)": "<rootDir>/src/studio/$1",
      "\\$shared/(.*)": "<rootDir>/src/shared/$1",
      "\\.(css|less)$": "identity-obj-proxy",
      "lodash-es": "lodash"
    },
    "setupFiles": [
      "./src/test/env.tsx"
    ],
    "automock": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/bundles",
      "<rootDir>/xeno",
      "<rootDir>/webpack"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.jest.json"
      }
    },
    "testURL": "http://localhost"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/studiojs/app.git"
  },
  "sideEffects": false,
  "author": "TheaterJS Oy",
  "license": "Proprietary",
