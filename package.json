{
  "name": "@theaterjs/app",
  "private": true,
  "version": "0.1.0-dev.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "flow-typed:install": "flow-typed install --flowVersion=0.53.0",
    "lint": "eslint ./src",
    "flow": "flow",
    "test": "jest",
    "prettify-and-lint": "run-s prettify lint",
    "prettify": "prettier --write \"src/**/*.js\"",
    "prepush": "run-p prettify-and-lint flow test",
    "lb:webpack:dev": "webpack --progress --config webpack/lb/development.babel.js --watch",
    "lb:dev": "nodemon -e jade --exec \"electron\" ./bundles/lb/index.js",
    "lb:run": "electron ./bundles/lb/index.js",
    "lf:webpack:dev": "webpack-dev-server --progress --config webpack/lf/development.babel.js",
    "studio:webpack:dev": "webpack-dev-server --progress --config webpack/studio/development.babel.js",
    "examples:webpack:dev": "webpack-dev-server --progress --config webpack/examples/development.babel.js",
    "build:dev": "npm-run-all --parallel lb:webpack:dev lf:webpack:dev studio:webpack:dev examples:webpack:dev",
    "open-examples": "opn http://localhost:9092/examples"
  },
  "jest": {
    "moduleNameMapper": {
      "\\$root/(.*)": "<rootDir>/$1",
      "\\$src/(.*)": "<rootDir>/src/$1",
      "\\$lb/(.*)": "<rootDir>/src/lb/$1",
      "\\$lf/(.*)": "<rootDir>/src/lf/$1",
      "\\$studio/(.*)": "<rootDir>/src/studio/$1",
      "\\$shared/(.*)": "<rootDir>/src/shared/$1"
    },
    "setupFiles": [
      "./src/test/env.js"
    ],
    "automock": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/theaterjs/app.git"
  },
  "author": "TheaterJS Oy",
  "license": "Proprietary",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.5",
