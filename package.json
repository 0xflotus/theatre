{
  "name": "@theaterjs/app",
  "private": true,
  "version": "0.1.0-dev.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "flow-typed:install": "flow-typed install --flowVersion=0.51.0",
    "lint": "eslint ./src",
    "flow": "flow",
    "test": "jest",
    "prepush": "run-p lint flow test",
    "lb:webpack:dev": "webpack --progress --config webpack/lb/development.babel.js --watch",
    "lb:dev": "nodemon -e jade --exec \"electron\" ./bundles/lb/index.js",
    "lb:run": "electron ./bundles/lb/index.js",
    "lf:webpack:dev": "webpack-dev-server --progress --config webpack/lf/development.babel.js",
    "studio:webpack:dev": "webpack-dev-server --progress --config webpack/studio/development.babel.js",
    "build:dev": "npm-run-all --parallel lb:webpack:dev lf:webpack:dev studio:webpack:dev"
  },
  "jest": {
    "moduleNameMapper": {
      "\\$root/(.*)": "<rootDir>/$1",
      "\\$src/(.*)": "<rootDir>/src/$1",
      "\\$lb/(.*)": "<rootDir>/src/lb/$1",
      "\\$lf/(.*)": "<rootDir>/src/lf/$1",
      "\\$studio/(.*)": "<rootDir>/src/studio/$1",
      "\\$shared/(.*)": "<rootDir>/src/shared/$1"
    },
    "setupFiles": [
      "./src/test/env.js"
    ],
    "automock": false
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/theaterjs/app.git"
  },
  "author": "TheaterJS Oy",
  "license": "Proprietary",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "deep-equal": "^1.0.1",
    "electron": "^1.6.11",
    "electron-debug": "^1.3.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^4.3.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-react": "^7.1.0",
    "exec-loader": "^3.0.0",
    "file-loader": "^0.11.2",
    "fs-extra": "^4.0.0",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "mock-fs": "^4.4.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^6.0.8",
    "postcss-hexrgba": "^1.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-nesting": "^4.0.1",
    "postcss-short": "^4.1.0",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-flow-types": "^0.1.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "recompose": "^0.24.0",
