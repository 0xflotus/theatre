{
  "name": "@theatrejs/app",
  "private": true,
  "version": "0.2.0-dev.3",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p . -t stylish -c tslint.json",
    "test": "jest",
    "tl:webpack:dev": "webpack-dev-server --progress --config webpack/tl/development.js",
    "tl:webpack:prod": "webpack --progress --config webpack/tl/production.js",
    "tl:publish": "cross-env NODE_ENV=production run-s tl:webpack:prod tl:publish:prepare",
    "tl:publish:prepare": "node ./scripts/tl/prepareBundle.js",
    "examples:webpack:dev": "webpack-dev-server --progress --config webpack/examples/development.js",
    "ci:check-types": "tsc -p ./ci.tsconfig.json --noEmit",
    "ci:check-types:watch": "tsc -p ./ci.tsconfig.json --noEmit --watch",
    "ci:type-tests": "tslint -c ./typeTests.tslint.json -p ./typeTests.tsconfig.json --format stylish ./src/**/*.typeTest.ts*",
    "prepush": "run-p ci:check-types test",
    "precommit": "run-p ci:check-formatting",
    "ci:check-formatting": "prettier --check \"{./src,./webpack,scripts,examples}/**/*.@(ts*|css|js)\"",
    "ci:fix-formatting": "prettier --write \"{./src,./webpack,scripts,examples}/**/*.@(ts*|css|js)\""
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleNameMapper": {
      "\\$root/(.*)": "<rootDir>/$1",
      "\\$src/(.*)": "<rootDir>/src/$1",
      "\\$shared/(.*)": "<rootDir>/src/shared/$1",
      "\\$tl/(.*)": "<rootDir>/src/tl/$1",
      "\\.(css)$": "identity-obj-proxy",
      "lodash-es": "lodash"
    },
    "setupFiles": [
      "./src/test/env.tsx"
    ],
    "automock": false,
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "babel-jest"
    },
    "moduleFileExtensions": [
      "tsx",
      "ts",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/bundles",
      "<rootDir>/xeno",
      "<rootDir>/webpack"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.jest.json"
      }
    },
    "testURL": "http://localhost"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/studiojs/app.git"
  },
  "sideEffects": false,
  "author": "TheaterJS Oy",
  "license": "Proprietary",
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/fuzzaldrin-plus": "0.6.0",
    "@types/html-webpack-plugin": "^3.2.0",
